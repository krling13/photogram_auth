<div class="col-md-8 col-md-offset-2">

    <div class="panel panel-default">
      <!-- Default panel contents -->
      <div class="panel-heading">All Users</div>

      <!-- List group -->
      <ul class="list-group">
        <% @users.each do |user| %>
        <li class="list-group-item">
          <a href="/users/<%=user.id%>"><%= user.username %></a>
        </li>
        <% end %>
      </ul>
    </div>
</div>
